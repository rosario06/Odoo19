<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Grupo: Usuario -->
    <record id="group_l10n_do_stock_user" model="res.groups">
        <field name="name">República Dominicana - Inventario: Usuario</field>
        <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
    </record>

    <!-- Grupo: Manager -->
    <record id="group_l10n_do_stock_manager" model="res.groups">
        <field name="name">República Dominicana - Inventario: Administrador</field>
        <field name="implied_ids" eval="[(4, ref('group_l10n_do_stock_user')), (4, ref('stock.group_stock_manager'))]"/>
    </record>

</odoo>

