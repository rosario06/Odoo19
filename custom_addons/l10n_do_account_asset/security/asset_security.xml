<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Grupo: Usuario de Activos Fijos -->
    <record id="group_account_asset_user" model="res.groups">
        <field name="name">República Dominicana - Activos Fijos: Usuario</field>
        <field name="implied_ids" eval="[(4, ref('account.group_account_user'))]"/>
    </record>

    <!-- Grupo: Administrador de Activos Fijos -->
    <record id="group_account_asset_manager" model="res.groups">
        <field name="name">República Dominicana - Activos Fijos: Administrador</field>
        <field name="implied_ids" eval="[(4, ref('group_account_asset_user')), (4, ref('account.group_account_manager'))]"/>
    </record>

</odoo>

