<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Secuencia para documentos e-CF -->
        <record id="seq_ecf_document" model="ir.sequence">
            <field name="name">Secuencia Documentos e-CF</field>
            <field name="code">ecf.document</field>
            <field name="prefix">ECF/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="use_date_range">False</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>

